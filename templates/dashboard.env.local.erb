# File managed by Puppet - do not edit
<%- scope['openondemand::dashboard_config'].keys.sort.each do |config| -%>
  <%- value = scope['openondemand::dashboard_config'][config] -%>
  <%- if value.to_s =~ /^(1|0)$/ -%>
<%= config %>=<%= value %>
  <%- elsif !!value == value -%>
<%= config %>=<%= value ? '1' : '0' %>
  <%- else -%>
<%= config %>="<%= value %>"
  <%- end -%>
<%- end -%>
